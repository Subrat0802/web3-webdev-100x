generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       Int    @id @default(autoincrement())
  name     String
  email    String
	password String
}

model Todo {
  id      Int      @id @default(autoincrement())
  title   String  
  done    Boolean  @default(false)
}